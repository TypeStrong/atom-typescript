{
  "name": "atom-typescript",
  "version": "7.12.0",
  "main": "./dist/main/atomts",
  "bin": {
    "atbuild": "./dist/main/bin/atbuild"
  },
  "preferGlobal": "true",
  "description": "The only TypeScript plugin you will ever need.",
  "scripts": {
    "test": "ntsc -p ./lib",
    "prepublish": "typings install"
  },
  "engines": {
    "atom": ">=0.199.0 <2.0.0",
    "node": "*"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provide"
      }
    },
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "consumedServices": {
    "snippets": {
      "versions": {
        "0.1.0": "consumeSnippets"
      }
    }
  },
  "homepage": "http://atom.io/packages/atom-typescript",
  "repository": {
    "type": "git",
    "url": "https://github.com/TypeStrong/atom-typescript.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TypeStrong/atom-typescript/issues"
  },
  "dependencies": {
    "atom-package-deps": "^2.0.3",
    "atom-space-pen-views": "^2.0.4",
    "babel": "^5.6.23",
    "basarat-text-buffer": "6.0.0",
    "d3": "^3.5.5",
    "detect-indent": "^4.0.0",
    "emissary": "^1.3.3",
    "escape-html": "^1.0.1",
    "findup": "^0.1.5",
    "fuzzaldrin": "^2.1.0",
    "glob": "^5.0.15",
    "htmltojsx": "0.2.4",
    "immutable": "^3.7.3",
    "json2dts": "0.0.1",
    "mkdirp": "^0.5.0",
    "ntypescript": "1.201511240604.1",
    "react": "^0.13.3",
    "season": "^5.1.4",
    "tsconfig": "^2.2.0",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "typings": "^0.2.3"
  },
  "package-deps": [
    "linter"
  ],
  "keywords": [
    "typescript",
    "javascript",
    "html"
  ]
}
