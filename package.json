{
  "name": "atom-typescript",
  "version": "11.0.10",
  "main": "./dist/main/atomts",
  "preferGlobal": true,
  "description": "The only TypeScript plugin you will ever need.",
  "atomTestRunner": "./spec/runner.js",
  "activationHooks": [
    "atom-typescript:grammar-used"
  ],
  "scripts": {
    "build": "tsc -p ./lib",
    "grammar": "ts-node scripts/grammar.ts",
    "prettier": "ts-node scripts/prettier.ts write",
    "prettier-check": "ts-node scripts/prettier.ts check",
    "typecheck": "tsc --noEmit -p ./lib && tsc --noEmit -p ./scripts && tsc --noEmit -p ./spec",
    "lint": "tslint --project ./lib && tslint --project ./spec",
    "test": "npm run typecheck && npm run lint && npm run prettier-check"
  },
  "engines": {
    "atom": ">=1.19.0 <2.0.0",
    "node": "*"
  },
  "configSchema": {
    "unusedAsInfo": {
      "title": "Show unused values with severity info",
      "description": "Show unused values with severity 'info' instead of 'error'",
      "type": "boolean",
      "default": true
    },
    "autocompletionSuggestionPriority": {
      "description": "Snippets etc have priority of 1. Requires Atom restart",
      "type": "integer",
      "default": 3
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "^2.0.0": "consumeLinter"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "hyperclick": {
      "versions": {
        "0.1.0": "hyperclickProvider"
      }
    },
    "intentions:list": {
      "versions": {
        "1.0.0": "provideIntentions"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    }
  },
  "homepage": "https://github.com/TypeStrong/atom-typescript",
  "repository": {
    "type": "git",
    "url": "https://github.com/TypeStrong/atom-typescript.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TypeStrong/atom-typescript/issues"
  },
  "dependencies": {
    "atom-package-deps": "^4.6.0",
    "atom-space-pen-views": "^2.0.4",
    "byline": "^5.0.0",
    "emissary": "^1.3.3",
    "escape-html": "^1.0.1",
    "fuzzaldrin": "^2.1.0",
    "jsx-render-dom": "0.1.2",
    "lodash": "^4.17.2",
    "resolve": "1.3.3",
    "tsconfig": "^6.0.0",
    "tslib": "^1.8.0",
    "typescript": "^2.4.2"
  },
  "devDependencies": {
    "@types/atom": "^1.21.8",
    "@types/byline": "^4.2.31",
    "@types/chai": "^4.0.4",
    "@types/cson": "0.0.30",
    "@types/emissary": "0.0.30",
    "@types/fuzzaldrin": "^2.1.0",
    "@types/lodash": "^4.14.51",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.47",
    "@types/resolve": "0.0.4",
    "@types/space-pen": "0.0.28",
    "atom-mocha-test-runner": "^1.0.1",
    "chai": "^4.1.2",
    "cson": "^4.1.0",
    "mz": "^2.1.0",
    "plist": "2.1.0",
    "popsicle": "9.1.0",
    "prettier": "^1.7.4",
    "ts-node": "3.2.1",
    "tslint": "^5.8.0"
  },
  "package-deps": [
    "atom-ide-ui:0.5.3"
  ],
  "keywords": [
    "typescript",
    "javascript",
    "html"
  ]
}
