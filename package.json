{
  "name": "atom-typescript",
  "version": "11.0.6",
  "main": "./dist/main/atomts",
  "preferGlobal": true,
  "description": "The only TypeScript plugin you will ever need.",
  "activationHooks": [
    "atom-typescript:grammar-used"
  ],
  "scripts": {
    "build": "tsc -p ./lib",
    "grammar": "ts-node scripts/grammar.ts",
    "prettier": "ts-node scripts/prettier.ts write",
    "test": "tsc --noEmit -p ./lib && tsc --noEmit -p ./scripts && ts-node scripts/prettier.ts check"
  },
  "engines": {
    "atom": ">=1.19.0 <2.0.0",
    "node": "*"
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "^2.0.0": "consumeLinter"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provide"
      }
    },
    "hyperclick.provider": {
      "versions": {
        "0.0.0": "hyperclickProvider"
      }
    },
    "intentions:list": {
      "versions": {
        "1.0.0": "provideIntentions"
      }
    }
  },
  "homepage": "https://github.com/TypeStrong/atom-typescript",
  "repository": {
    "type": "git",
    "url": "https://github.com/TypeStrong/atom-typescript.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TypeStrong/atom-typescript/issues"
  },
  "dependencies": {
    "atom-package-deps": "^4.6.0",
    "atom-space-pen-views": "^2.0.4",
    "byline": "^5.0.0",
    "emissary": "^1.3.3",
    "escape-html": "^1.0.1",
    "fuzzaldrin": "^2.1.0",
    "jsx-render-dom": "0.1.2",
    "lodash": "^4.17.2",
    "resolve": "1.3.3",
    "tsconfig": "^6.0.0",
    "tslib": "1.7.1",
    "typescript": "~2.4.2",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "@types/atom": "0.0.38",
    "@types/byline": "^4.2.31",
    "@types/cson": "0.0.30",
    "@types/fuzzaldrin": "^2.1.0",
    "@types/lodash": "^4.14.51",
    "@types/resolve": "0.0.4",
    "cson": "^4.1.0",
    "mz": "^2.1.0",
    "plist": "2.1.0",
    "popsicle": "9.1.0",
    "prettier": "1.5.3",
    "ts-node": "3.2.1"
  },
  "package-deps": [
    "linter:2.2.0"
  ],
  "keywords": [
    "typescript",
    "javascript",
    "html"
  ]
}
